    Chnager l'envoie de dico en bytes avec : @njit
def pack_cells_to_numpy(cells_x, cells_y, cells_r, cells_g, cells_b, cells_a):
    n = len(cells_x)
    buf = np.zeros(n * 8, dtype=np.uint8)

    k = 0
    for i in range(n):
        x = cells_x[i]
        y = cells_y[i]
        buf[k]   = (x >> 8) & 0xFF
        buf[k+1] = x & 0xFF
        buf[k+2] = (y >> 8) & 0xFF
        buf[k+3] = y & 0xFF
        buf[k+4] = cells_r[i]
        buf[k+5] = cells_g[i]
        buf[k+6] = cells_b[i]
        buf[k+7] = cells_a[i]
        k += 8

    return buf

packet = bytearray()
packet += struct.pack("!B", 1)
packet += struct.pack("!H", n)
packet += pack_cells_to_numpy(x, y, r, g, b, a).tobytes()

socket.send(packet)


Reception : id_msg = struct.unpack("!B", data[0:1])[0]
count  = struct.unpack("!H", data[1:3])[0]

offset = 3
cells = []

for _ in range(count):
    x, y, r, g, b, a = struct.unpack("!hhBBBB", data[offset:offset+8])
    cells.append((x, y, (r, g, b, a)))
    offset += 8
